{
  "version": 2,
  "functions": {
    "main.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./"
  },
  "build": {
    "env": {
      "PYTHONPATH": "./"
    }
  },
  "regions": ["iad1"],
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}
